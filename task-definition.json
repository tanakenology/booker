{
  "family": "booker-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "booker-app",
      "image": "booker-app",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": ["booker"],
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:118815212218:secret:booker-r6kNcX",
          "name": "booker"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "/ecs/booker",
            "awslogs-region": "ap-northeast-1",
            "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "booker-chrome",
      "image": "booker-chrome",
      "portMappings": [
        {
          "containerPort": 4444,
          "hostPort": 4444,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
